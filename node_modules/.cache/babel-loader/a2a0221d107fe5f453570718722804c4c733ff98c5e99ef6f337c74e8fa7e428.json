{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport NavBar from '@/components/inc/NavBar.vue';\nimport SideBar from '@/components/inc/SideBar.vue';\nexport default {\n  name: 'ProductsView',\n  components: {\n    NavBar,\n    SideBar\n  },\n  mounted() {\n    this.getDataPagina(1);\n  },\n  created() {\n    var req = {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }; //Validação da API\n    // PESQUISA NO BANCO DE DADOS//\n    axios.get(\"http://localhost:8686/api/product\", req).then(res => {\n      this.bd = res.data;\n      this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n      this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\n      console.log(res);\n    }).catch(err => {\n      var msgErro = err.response.data.err;\n      this.error = msgErro;\n    }); //consulta de usuarios\n  },\n\n  data() {\n    return {\n      //consulta de dados do BD//\n      bd: [],\n      filteredBd: [],\n      //Variaveis\n      id: -1,\n      produto: \"\",\n      vlrvenda: \"\",\n      categoria: \"\",\n      status: \"A\",\n      error: undefined,\n      //ID para exclusão\n      deleteItemId: -1,\n      //Campos para Pesquisa\n      bCategoria: \"\",\n      bStatus: \"A\",\n      //Controle de Elementos HTML\n      showModal: false,\n      showModalDelete: false,\n      showModalUpdate: false,\n      showElement: false,\n      /*PAGINAÇÃO*/\n      elementosPorPagina: 10,\n      dadosPaginados: [],\n      paginaAtual: 1\n    };\n  },\n  props: {},\n  computed: {},\n  methods: {\n    mostrarElemento: function () {\n      this.showElement = !this.showElement;\n    },\n    totalPaginas() {\n      return Math.ceil(this.filteredBd.length / this.elementosPorPagina);\n    },\n    getDataPagina(numPagina) {\n      this.paginaAtual = numPagina;\n      this.dadosPaginados = [];\n      var ini = numPagina * this.elementosPorPagina - this.elementosPorPagina;\n      var fin = numPagina * this.elementosPorPagina;\n      this.dadosPaginados = this.filteredBd.slice(ini, fin);\n    },\n    getPreviusPage() {\n      if (this.paginaAtual > 1) {\n        this.paginaAtual--;\n      }\n      this.getDataPagina(this.paginaAtual);\n    },\n    getNextPage() {\n      if (this.paginaAtual < this.totalPaginas()) {\n        this.paginaAtual++;\n      }\n      this.getDataPagina(this.paginaAtual);\n    },\n    isActive(numPagina) {\n      return numPagina == this.paginaAtual ? 'is-current' : '';\n    },\n    ShowModal() {\n      this.showModal = true;\n    },\n    ShowModalDelete(id) {\n      console.log(\"Id do Item:\" + id);\n      this.deleteItemId = id;\n      this.showModalDelete = true;\n    },\n    ShowModalUpdate(id) {\n      console.log(\"Id do Item:\" + id);\n      this.deleteItemId = id;\n      this.showModalUpdate = true;\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }; //validação da API\n      axios.get(\"http://localhost:8686/api/product/\" + this.deleteItemId, req).then(res => {\n        console.log(res);\n        this.produto = res.data.produto;\n        this.vlrvenda = res.data.vlrvenda;\n        this.categoria = res.data.categoria_id;\n        this.id = res.data.id;\n        this.status = res.data.status;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    },\n    HideModal() {\n      //fechar moldais\n      this.showModal = false;\n      this.showModalDelete = false;\n      this.showModalUpdate = false;\n\n      //limpar dados\n\n      this.id = \"\";\n      this.nome = \"\";\n      this.usuario = \"\";\n      this.email = \"\";\n    },\n    //===============================CRUD===============================//\n    //inclusão\n    register() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      };\n      axios.post(\"http://localhost:8686/api/category\", {\n        categoria: this.categoria,\n        status: \"A\"\n      }, req).then(res => {\n        this.$router.push({\n          name: 'categories'\n        });\n        console.log(res);\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n        console.log(err);\n      });\n    },\n    //Exclusão\n    deleteItem() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      };\n      axios.delete(\"http://localhost:8686/api/category/\" + this.deleteItemId, req).then(res => {\n        console.log(res);\n        this.showModalDelete = false;\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/category\", req).then(res => {\n          this.bd = res.data;\n          this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n        });\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //\n\n    //Edição\n    updateItem() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }; //validação da API\n      axios.put(\"http://localhost:8686/api/product/\" + this.deleteItemId, {\n        produto: this.produto,\n        vlrvenda: this.vlrvenda,\n        categoria_id: this.categoria,\n        id: this.id,\n        status: this.status\n      }, req).then(res => {\n        this.showModalUpdate = false;\n\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/product\", req).then(res => {\n          this.bd = res.data;\n          this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n        });\n        console.log(res);\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n        console.log(err);\n      });\n    },\n    buscarService: function () {\n      this.filteredBd = this.bd;\n      if (this.bCategoria != '' || this.bCategoria != ' ' && this.bStatus != '' || this.bStatus != ' ') {\n        this.filteredBd = this.bd.filter(bd => bd.categoria.match(this.bCategoria) && bd.status.match(this.bStatus));\n      } else {\n        this.filteredBd = this.bd;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","NavBar","SideBar","name","components","mounted","getDataPagina","created","req","headers","Authorization","localStorage","getItem","get","then","res","bd","data","filteredBd","filter","status","match","bStatus","dadosPaginados","slice","elementosPorPagina","console","log","catch","err","msgErro","response","error","id","produto","vlrvenda","categoria","undefined","deleteItemId","bCategoria","showModal","showModalDelete","showModalUpdate","showElement","paginaAtual","props","computed","methods","mostrarElemento","totalPaginas","Math","ceil","length","numPagina","ini","fin","getPreviusPage","getNextPage","isActive","ShowModal","ShowModalDelete","ShowModalUpdate","categoria_id","HideModal","nome","usuario","email","register","post","$router","push","deleteItem","delete","updateItem","put","buscarService"],"sources":["C:\\Users\\naldo\\OneDrive\\Documents\\GitHub\\appbarber\\barbervue\\src\\views\\ProdutosView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--Barras de navegação horizontal e vertical-->\r\n    <NavBar />\r\n    <Side-bar />\r\n    <!--Conteúdo-->\r\n    <div class=\"main-content\">\r\n      <main>\r\n        <div class=\"page-header\">\r\n          <div>\r\n            <h2>Cadastro de Produtos</h2>\r\n          </div>\r\n          <!--Migalhas de Pão - Navegalçao Histórica-->\r\n          <nav class=\"breadcrumb is-right is-small\" aria-label=\"breadcrumbs\">\r\n            <ul>\r\n              <li><a href=\"#\">Home</a></li>\r\n              <li><a href=\"#\">Cadastros</a></li>\r\n              <li class=\"is-active\"><a href=\"#\" aria-current=\"page\">Produtos</a></li>\r\n            </ul>\r\n          </nav>\r\n        </div>\r\n        <!--Cabeçalho de Pesquisa -->\r\n        <div class=\"card\" id=\"card-pesquisa\">\r\n          <header class=\"card-header\" id=\"header-card\">\r\n            <p class=\"card-header-title\">\r\n              <span class=\"icon\">\r\n                <i class=\"las la-filter\"></i>\r\n              </span>\r\n              <span>Filtros</span>\r\n            </p>\r\n            <button class=\"card-header-icon\" aria-label=\"more options\" @click=\"mostrarElemento\">\r\n              <span class=\"icon\">\r\n                <i :class=\"{ 'las la-angle-up': showElement, 'las la-angle-down': !showElement }\" aria-hidden=\"true\"></i>\r\n              </span>\r\n            </button>\r\n          </header>\r\n          <div class=\"card-content\" v-show=\"showElement\">\r\n            <div class=\"content\">\r\n\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <input class=\"input is-small\" type=\"text\" placeholder=\"Produto\" v-model=\"bProduto\">\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <input class=\"input is-small\" type=\"text\" placeholder=\"Categoria\" v-model=\"bCategoria\">\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <div class=\"select is-small\">\r\n                      <select v-model=\"bStatus\">\r\n                        <option selected value=\"A\">Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                  <button class=\"button is-info is-small\" @click=\"buscaAvc\">Pesquisar</button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!--Pesquisas-->\r\n        </div>\r\n        <!--Cabeçalho do Formulario/tabela-->\r\n        <div class=\"card\">\r\n          <header class=\"card-header is-small\" id=\"header-card-form\">\r\n            <p class=\"card-header-title\">\r\n              <button class=\"button is-info is-outlined\" @click=\"ShowModal\">Adicionar</button>\r\n            </p>\r\n          </header>\r\n          <div class=\"card-content\">\r\n            <div class=\"content\">\r\n              <table class=\"table is-hoverable is-fullwidth\">\r\n                <thead class=\"is-fixed\">\r\n                  <tr>\r\n                    <th>ID</th>\r\n                    <th>Produto</th>\r\n                    <th>Valor</th>\r\n                    <th>Categoria</th>\r\n                    <th>Status</th>\r\n                    <th>Ações</th>\r\n                  </tr>\r\n                </thead>\r\n\r\n                <tbody>\r\n                  <tr v-for=\"bd in filteredBd\" :key=\"bd.id\">\r\n                    <td>{{ bd.id }}</td>\r\n                    <td>{{ bd.produto }}</td>\r\n                    <td>{{ bd.vlrvenda }}</td>\r\n                    <td>{{ bd.categoria_id }}</td>\r\n                    <td>{{ bd.status }}</td>\r\n                    <td>\r\n                      <button class=\"button is-warning is-small\" @click=\"ShowModalUpdate(bd.id)\">Editar</button>\r\n                      <button class=\"button is-danger is-small\" @click=\"ShowModalDelete(bd.id)\">Deletar</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n              <nav class=\"pagination is-centered is-rounded\" role=\"navigation\" aria-label=\"pagination\">\r\n                <a class=\"pagination-previous is-centered\" v-on:click=\"getPreviusPage()\"> <i\r\n                    class=\"las la-angle-left\"></i></a>\r\n                <a class=\"pagination-next\" v-on:click=\"getNextPage()\"><i class=\"las la-angle-right\"></i></a>\r\n                <ul class=\"pagination-list\">\r\n                  <li><a v-for=\"pagina in totalPaginas()\" :key=pagina.id v-on:click=\"getDataPagina(pagina)\"\r\n                      class=\"pagination-link\" v-bind:class=\"isActive(pagina)\" aria-label=\"Goto page 1\">{{ pagina }}</a>\r\n                  </li>\r\n                </ul>\r\n              </nav>\r\n            </div>\r\n          </div>\r\n          <footer class=\"card-footer\">\r\n\r\n          </footer>\r\n        </div>\r\n\r\n\r\n        <!--MODAL - Cadastro de Usuários-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModal }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Novo Produto</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <div v-if=\"error != undefined\">\r\n                      <div class=\"notification is-danger\">\r\n                        {{ error }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <input class=\"input\" type=\"text\" placeholder=\"Produto\" v-model=\"produto\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <input class=\"input\" type=\"text\" placeholder=\"Valor\" v-model=\"vlrvenda\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <div class=\"select\">\r\n                      <select v-model=\"categoria\">\r\n                        <option value=\"A\">Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"register\">Cadastrar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - Cadastro de Usuários-->\r\n        <!--MODAL - DELETE-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModalDelete }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n\r\n\r\n            <!-- Content ... -->\r\n\r\n\r\n            <div class=\"card\">\r\n              <header class=\"card-header\">\r\n                <p class=\"card-header-title is-centered\">\r\n                  ATENÇÃO\r\n                </p>\r\n\r\n              </header>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\">\r\n                  Deseja realmente Deletar este Produto?\r\n\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer\">\r\n                <a href=\"#\" class=\"card-footer-item\" @click=\"deleteItem\">Deletar</a>\r\n                <a href=\"#\" class=\"card-footer-item\" @click=\"HideModal\">Cancelar</a>\r\n              </footer>\r\n            </div>\r\n\r\n\r\n          </div>\r\n        </div> <!--MODAL - DELTE-->\r\n\r\n        <!--MODAL - ALTERAR-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModalUpdate }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Editar Produto</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n\r\n                <div class=\"field-body\">\r\n\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <div v-if=\"error != undefined\">\r\n\r\n                      <div class=\"notification is-danger\">\r\n                        {{ error }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <input class=\"input\" type=\"text\" v-model=\"produto\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <input class=\"input\" type=\"text\" v-model=\"vlrvenda\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <input class=\"input\" type=\"text\" v-model=\"categoria\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <div class=\"select\">\r\n                      <select v-model=\"status\">\r\n                        <option value=\"A\">Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"updateItem\">Salvar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - ALTERAR-->\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\n\r\nexport default {\r\n  name: 'ProductsView',\r\n  components: {\r\n    NavBar,\r\n    SideBar,\r\n\r\n  },\r\n  mounted() {\r\n    this.getDataPagina(1);\r\n  },\r\n  created() {\r\n    var req = {\r\n      headers: {\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n      },\r\n    };//Validação da API\r\n    // PESQUISA NO BANCO DE DADOS//\r\n    axios\r\n      .get(\"http://localhost:8686/api/product\", req)\r\n      .then((res) => {\r\n        this.bd = res.data;\r\n        this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\r\n        this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\r\n        console.log(res);\r\n      })\r\n      .catch((err) => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro;\r\n      });//consulta de usuarios\r\n\r\n\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      //consulta de dados do BD//\r\n      bd: [],\r\n      filteredBd: [],\r\n\r\n      //Variaveis\r\n      id: -1,\r\n      produto: \"\",\r\n      vlrvenda: \"\",\r\n      categoria: \"\",\r\n      status: \"A\",\r\n      error: undefined,\r\n      //ID para exclusão\r\n      deleteItemId: -1,\r\n      //Campos para Pesquisa\r\n      bCategoria: \"\",\r\n      bStatus: \"A\",\r\n      //Controle de Elementos HTML\r\n      showModal: false,\r\n      showModalDelete: false,\r\n      showModalUpdate: false,\r\n      showElement: false,\r\n      /*PAGINAÇÃO*/\r\n      elementosPorPagina: 10,\r\n      dadosPaginados: [],\r\n      paginaAtual: 1\r\n    }\r\n  },\r\n  props: {\r\n\r\n  },\r\n  computed: {\r\n\r\n  },\r\n\r\n  methods: {\r\n\r\n    mostrarElemento: function () {\r\n      this.showElement = !this.showElement\r\n    },\r\n    totalPaginas() {\r\n      return Math.ceil(this.filteredBd.length / this.elementosPorPagina)\r\n    },\r\n    getDataPagina(numPagina) {\r\n      this.paginaAtual = numPagina;\r\n      this.dadosPaginados = [];\r\n      var ini = (numPagina * this.elementosPorPagina) - this.elementosPorPagina;\r\n      var fin = (numPagina * this.elementosPorPagina);\r\n      this.dadosPaginados = this.filteredBd.slice(ini, fin);\r\n    },\r\n    getPreviusPage() {\r\n      if (this.paginaAtual > 1) {\r\n        this.paginaAtual--;\r\n      }\r\n      this.getDataPagina(this.paginaAtual)\r\n    },\r\n    getNextPage() {\r\n      if (this.paginaAtual < this.totalPaginas()) {\r\n        this.paginaAtual++;\r\n      }\r\n      this.getDataPagina(this.paginaAtual)\r\n    },\r\n    isActive(numPagina) {\r\n      return numPagina == this.paginaAtual ? 'is-current' : ''\r\n    },\r\n    ShowModal() {\r\n      this.showModal = true;\r\n    },\r\n    ShowModalDelete(id) {\r\n      console.log(\"Id do Item:\" + id);\r\n      this.deleteItemId = id;\r\n      this.showModalDelete = true;\r\n    },\r\n    ShowModalUpdate(id) {\r\n      console.log(\"Id do Item:\" + id);\r\n      this.deleteItemId = id;\r\n      this.showModalUpdate = true;\r\n\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };//validação da API\r\n      axios\r\n        .get(\"http://localhost:8686/api/product/\" + this.deleteItemId, req)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.produto = res.data.produto;\r\n          this.vlrvenda = res.data.vlrvenda;\r\n          this.categoria = res.data.categoria_id;\r\n          this.id = res.data.id;\r\n          this.status = res.data.status\r\n\r\n        }).catch(err => {\r\n          console.log(err.response);\r\n        });\r\n\r\n    },\r\n    HideModal() {\r\n      //fechar moldais\r\n      this.showModal = false;\r\n      this.showModalDelete = false;\r\n      this.showModalUpdate = false;\r\n\r\n      //limpar dados\r\n\r\n      this.id = \"\";\r\n      this.nome = \"\";\r\n      this.usuario = \"\";\r\n      this.email = \"\"\r\n\r\n    },\r\n    //===============================CRUD===============================//\r\n    //inclusão\r\n    register() {\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };\r\n      axios\r\n        .post(\"http://localhost:8686/api/category\", {\r\n          categoria: this.categoria,\r\n          status: \"A\"\r\n        }, req)\r\n        .then(res => {\r\n          this.$router.push({ name: 'categories' });\r\n          console.log(res);\r\n        })\r\n        .catch(err => {\r\n          var msgErro = err.response.data.err;\r\n          this.error = msgErro\r\n          console.log(err)\r\n        })\r\n\r\n    },\r\n    //Exclusão\r\n    deleteItem() {\r\n\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };\r\n\r\n      axios\r\n        .delete(\"http://localhost:868