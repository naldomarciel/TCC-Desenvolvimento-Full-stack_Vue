v>\r\n        </div>\r\n\r\n        <!--MODAL - Cadastro de Usuários-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModal }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card \">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Agendar</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input\" type=\"date\" placeholder=\"Data\" v-model=\"dataSelecionada\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input\" type=\"time\">\r\n                    </p>\r\n                  </div>\r\n\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input input-small\" type=\"email\" placeholder=\"contato@mail.com\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-envelope\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"register\">Cadastrar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - Cadastro de Usuários-->\r\n\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport FullCalendar from '@fullcalendar/vue3'\r\nimport dayGridPlugin from '@fullcalendar/daygrid'\r\nimport interactionPlugin from '@fullcalendar/interaction'\r\nimport timeGridPlugin from '@fullcalendar/timegrid'\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\n\r\n\r\nexport default {\r\n  components: {\r\n    FullCalendar, // make the <FullCalendar> tag available\r\n    NavBar,\r\n    SideBar,\r\n  },\r\n  data() {\r\n\r\n    return {\r\n      calendarOptions: {\r\n        plugins: [dayGridPlugin, timeGridPlugin, interactionPlugin],\r\n        initialView: 'dayGridMonth',\r\n        weekends: true, // initial value,\r\n        editable: true,\r\n        selectable: true,\r\n        selectMirror: true,\r\n        dayMaxEvents: true,\r\n        dateClick: this.handleDateClick,\r\n\r\n        unselect: this.handleSelect,\r\n        eventClick: function (info) {\r\n          alert(\r\n            info.event.id + \" - \"+\r\n            info.event.title )\r\n        },\r\n        locale: 'pt-br',\r\n        headerToolbar: {\r\n          left: 'prev,next',\r\n          center: 'title',\r\n          right: 'dayGridMonth,timeGridWeek,timeGridDay'// user can switch between the two\r\n        },\r\n\r\n        events:\r\n          \"http://localhost:8686/api/appointment\"\r\n\r\n      },\r\n\r\n      dados: [],\r\n      filteredUsuarios: [],\r\n      showModal: false,\r\n      showModalDelete: false,\r\n      showModalUpdate: false,\r\n      showElement: false,\r\n      dataSelecionada: \"\",\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    handleDateClick(e) {\r\n      this.dataSelecionada=e.dateStr\r\n      this.showModal=true;\r\n      },\r\n    handleSelect(e) {\r\n      console.log(e);\r\n    },\r\n    toggleWeekends: function () {\r\n      this.calendarOptions.weekends = !this.calendarOptions.weekends // toggle the boolean!\r\n    },\r\n    HideModal() {\r\n      //fechar moldais\r\n      this.showModal = false;\r\n      this.showModalDelete = false;\r\n      this.showModalUpdate = false;\r\n\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n"],"mappings":"AAgGA,OAAOA,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,aAAY,MAAO,uBAAsB;AAChD,OAAOC,iBAAgB,MAAO,2BAA0B;AACxD,OAAOC,cAAa,MAAO,wBAAuB;AAClD,OAAOC,MAAK,MAAO,6BAA4B;AAC/C,OAAOC,OAAM,MAAO,8BAA6B;AAGjD,eAAe;EACbC,UAAU,EAAE;IACVN,YAAY;IAAE;IACdI,MAAM;IACNC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IAEL,OAAO;MACLC,eAAe,EAAE;QACfC,OAAO,EAAE,CAACR,aAAa,EAAEE,cAAc,EAAED,iBAAiB,CAAC;QAC3DQ,WAAW,EAAE,cAAc;QAC3BC,QAAQ,EAAE,IAAI;QAAE;QAChBC,QAAQ,EAAE,IAAI;QACdC,UAAU,EAAE,IAAI;QAChBC,YAAY,EAAE,IAAI;QAClBC,YAAY,EAAE,IAAI;QAClBC,SAAS,EAAE,IAAI,CAACC,eAAe;QAE/BC,QAAQ,EAAE,IAAI,CAACC,YAAY;QAC3BC,UAAU,EAAE,SAAAA,CAAUC,IAAI,EAAE;UAC1BC,KAAK,CACHD,IAAI,CAACE,KAAK,CAACC,EAAC,GAAI,KAAK,GACrBH,IAAI,CAACE,KAAK,CAACE,KAAI,CAAE;QACrB,CAAC;QACDC,MAAM,EAAE,OAAO;QACfC,aAAa,EAAE;UACbC,IAAI,EAAE,WAAW;UACjBC,MAAM,EAAE,OAAO;UACfC,KAAK,EAAE,uCAAuC;QAChD,CAAC;;QAEDC,MAAM,EACJ;MAEJ,CAAC;MAEDC,KAAK,EAAE,EAAE;MACTC,gBAAgB,EAAE,EAAE;MACpBC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE,KAAK;MACtBC,eAAe,EAAE,KAAK;MACtBC,WAAW,EAAE,KAAK;MAClBC,eAAe,EAAE;IAEnB;EACF,CAAC;EACDC,OAAO,EAAE;IACPtB,eAAeA,CAACuB,CAAC,EAAE;MACjB,IAAI,CAACF,eAAe,GAACE,CAAC,CAACC,OAAM;MAC7B,IAAI,CAACP,SAAS,GAAC,IAAI;IACnB,CAAC;IACHf,YAAYA,CAACqB,CAAC,EAAE;MACdE,OAAO,CAACC,GAAG,CAACH,CAAC,CAAC;IAChB,CAAC;IACDI,cAAc,EAAE,SAAAA,CAAA,EAAY;MAC1B,IAAI,CAACpC,eAAe,CAACG,QAAO,GAAI,CAAC,IAAI,CAACH,eAAe,CAACG,Q