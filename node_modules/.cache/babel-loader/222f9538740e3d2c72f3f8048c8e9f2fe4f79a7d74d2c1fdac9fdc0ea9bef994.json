{"ast":null,"code":"import FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport interactionPlugin from '@fullcalendar/interaction';\nimport timeGridPlugin from '@fullcalendar/timegrid';\nimport NavBar from '@/components/inc/NavBar.vue';\nimport SideBar from '@/components/inc/SideBar.vue';\nexport default {\n  components: {\n    FullCalendar,\n    // make the <FullCalendar> tag available\n    NavBar,\n    SideBar\n  },\n  data() {\n    return {\n      calendarOptions: {\n        plugins: [dayGridPlugin, timeGridPlugin, interactionPlugin],\n        initialView: 'dayGridMonth',\n        weekends: true,\n        // initial value,\n        editable: true,\n        selectable: true,\n        selectMirror: true,\n        dayMaxEvents: true,\n        dateClick: this.handleDateClick,\n        select: this.handleSelect,\n        unselect: this.handleSelect,\n        locale: 'pt-br',\n        headerToolbar: {\n          left: 'prev,next',\n          center: 'title',\n          right: 'dayGridMonth,timeGridWeek,timeGridDay' // user can switch between the two\n        },\n\n        events: \"http://localhost:8686/api/appointment\"\n      },\n      dados: [],\n      filteredUsuarios: [],\n      showModal: false,\n      showModalDelete: false,\n      showModalUpdate: false,\n      showElement: false\n    };\n  },\n  methods: {\n    handleDateClick(e) {\n      console.log(e.dateClick);\n    },\n    handleSelect(e) {\n      console.log(e);\n    },\n    toggleWeekends: function () {\n      this.calendarOptions.weekends = !this.calendarOptions.weekends; // toggle the boolean!\n    }\n  }\n};","map":{"version":3,"names":["FullCalendar","dayGridPlugin","interactionPlugin","timeGridPlugin","NavBar","SideBar","components","data","calendarOptions","plugins","initialView","weekends","editable","selectable","selectMirror","dayMaxEvents","dateClick","handleDateClick","select","handleSelect","unselect","locale","headerToolbar","left","center","rig