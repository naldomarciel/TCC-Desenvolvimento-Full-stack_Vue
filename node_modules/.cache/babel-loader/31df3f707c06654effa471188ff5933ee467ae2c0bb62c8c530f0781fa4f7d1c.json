al-->\r\n    <NavBar />\r\n    <Side-bar />\r\n    <!--Conteúdo-->\r\n    <div class=\"main-content\">\r\n      <main>\r\n        <div class=\"page-header\">\r\n          <div>\r\n            <h2>Cadastro de Usuários</h2>\r\n          </div>\r\n         <!--Migalhas de Pão - Navegalçao Histórica-->\r\n          <nav class=\"breadcrumb is-right is-small\" aria-label=\"breadcrumbs\">\r\n            <ul>\r\n              <li><a href=\"#\">Home</a></li>\r\n              <li><a href=\"#\">Cadastros</a></li>\r\n              <li class=\"is-active\"><a href=\"#\" aria-current=\"page\">Usuário</a></li>\r\n            </ul>\r\n          </nav>\r\n        </div>\r\n         <!--Cabeçalho de Pesquisa -->\r\n        <div class=\"card\">\r\n          <header class=\"card-header\" id=\"header-card\">\r\n            <p class=\"card-header-title\">\r\n              <span class=\"icon\">\r\n                <i class=\"las la-filter\"></i>\r\n                <i class=\"las la-filter-solid\"></i>\r\n              </span>\r\n              <span>Filtros</span>\r\n            </p>\r\n            <button class=\"card-header-icon\" aria-label=\"more options\">\r\n              <span class=\"icon\">\r\n                <i class=\"las la-angle-down\" aria-hidden=\"true\"></i>\r\n              </span>\r\n            </button>\r\n          </header>\r\n           <!--Pesquisas-->\r\n        </div>\r\n        <!--Cabeçalho do Formulario/tabela-->\r\n        <div class=\"card\">\r\n          <header class=\"card-header small\">\r\n            <p class=\"card-header-title\">\r\n              <button class=\"button is-info is-outlined\" @click=\"ShowModal\">Adicionar</button>\r\n            </p>\r\n         </header>\r\n          <div class=\"card-content\">\r\n            <div class=\"content\">\r\n              <table class=\"table is-hoverable is-fullwidth\">\r\n                <thead class=\"is-fixed\">\r\n                  <tr>\r\n                    <th>ID</th>\r\n                    <th>Usuário</th>\r\n                    <th>Nome</th>\r\n                    <th>E-mail</th>\r\n                    <th>Status</th>\r\n                    <th>Ações</th>\r\n                  </tr>\r\n                </thead>\r\n\r\n                <tbody>\r\n                  <tr v-for=\"usuario in usuarios\" :key=\"usuario.id\">\r\n                    <td>{{ usuario.id }}</td>\r\n                    <td>{{ usuario.usuario }}</td>\r\n                    <td>{{ usuario.nome }}</td>\r\n                    <td>{{ usuario.email }}</td>\r\n                    <td>{{ usuario.status }}</td>\r\n                    <td>\r\n                      <button class=\"button is-warning is-small\">Editar</button>\r\n                      <button class=\"button is-danger is-small\">Deletar</button>\r\n                    </td>\r\n\r\n\r\n                  </tr>\r\n\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n          <footer class=\"card-footer\">\r\n            \r\n          </footer>\r\n        </div>\r\n\r\n\r\n        <!--MODAL - Cadastro de Usuários-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModal }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Novo Usuário</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n      \r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input\" type=\"text\" placeholder=\"Nome Completo\"  v-model=\"usuario\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-user\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input input-small\" type=\"email\" placeholder=\"contato@mail.com\" v-model=\"email\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-envelope\"></i>\r\n                      </span>\r\n                    </p>\r\n\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input input-small\" type=\"text\" placeholder=\"Usuário\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-user\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n       \r\n                  <div class=\"field\">\r\n                    <p class=\"control has-icons-left\">\r\n                      <input class=\"input\" type=\"password\" placeholder=\"Senha\" v-model=\"senha\" >\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-lock\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n\r\n\r\n                </div>\r\n              </div>\r\n        \r\n\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"register\">Cadastrar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\n\r\nexport default {\r\n  name: 'UsersView',\r\n  components: {\r\n    NavBar,\r\n    SideBar,\r\n\r\n  },\r\n  created() {\r\n    var req = {\r\n      headers: {\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n      },\r\n    };\r\n\r\n    axios\r\n      .get(\"http://localhost:8686/api/user\", req)\r\n      .then((res) => {\r\n        this.usuarios = res.data;\r\n        console.log(res);\r\n      })\r\n      .catch((err) => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro;\r\n      });\r\n  },\r\n  data() {\r\n    return {\r\n      usuarios: [],\r\n      showModal: false,\r\n      usuario: \"\",\r\n      email: \"\",\r\n      senha: \"\",\r\n      error: undefined,\r\n    }\r\n  },\r\n  methods: {\r\n    ShowModal() {\r\n      this.showModal = true;\r\n    },\r\n    HideModal() {\r\n      this.showModal = false;\r\n    },\r\n    register() {\r\n      axios.post(\"http://localhost:8686/api/user\", {\r\n        usuario: this.usuario,\r\n        email: this.email,\r\n        senha: this.senha,\r\n        nome: this.usuario,\r\n        status: \"A\"\r\n      }).then(res => {\r\n        this.$router.push({ name: 'home' });\r\n        console.log(res);\r\n      }).catch(err => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro\r\n      })\r\n\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"mappings":";AAgKA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,6BAA4B;AAC/C,OAAOC,OAAM,MAAO,8BAA6B;AAEjD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVH,MAAM;IACNC;EAEF,CAAC;EAC