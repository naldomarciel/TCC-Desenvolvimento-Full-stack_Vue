ção horizontal e vertical-->\r\n    <NavBar />\r\n    <Side-bar />\r\n    <!--Conteúdo-->\r\n    <div class=\"main-content\">\r\n      <main>\r\n        <div class=\"page-header\">\r\n          <div>\r\n            <h2>Cadastro de Serviços</h2>\r\n          </div>\r\n          <!--Migalhas de Pão - Navegalçao Histórica-->\r\n          <nav class=\"breadcrumb is-right is-small\" aria-label=\"breadcrumbs\">\r\n            <ul>\r\n              <li><a href=\"#\">Home</a></li>\r\n              <li><a href=\"#\">Cadastros</a></li>\r\n              <li class=\"is-active\"><a href=\"#\" aria-current=\"page\">Serviço</a></li>\r\n            </ul>\r\n          </nav>\r\n        </div>\r\n        <!--Cabeçalho de Pesquisa -->\r\n\r\n\r\n        <FullCalendar :options=\"calendarOptions\" />\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\n\r\nexport default {\r\n  name: 'AgendamentosView',\r\n  components: {\r\n    NavBar,\r\n    SideBar,\r\n\r\n  },\r\n  created() {\r\n    var req = {\r\n      headers: {\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n      },\r\n    };//Validação da API\r\n    // PESQUISA NO BANCO DE DADOS//\r\n    axios\r\n      .get(\"http://localhost:8686/api/service\", req)\r\n      .then((res) => {\r\n        this.servicos = res.data;\r\n        this.filteredServicos = res.data.filter(service => service.status.match(this.bStatus));\r\n        console.log(res);\r\n      })\r\n      .catch((err) => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro;\r\n      });//consulta de usuarios\r\n\r\n\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      //consulta de dados do BD//\r\n      servicos: [],\r\n      filteredServicos: [],\r\n\r\n      //Variaveis\r\n      id: -1,\r\n      servico: \"\",\r\n      valor: \"\",\r\n      status: \"A\",\r\n      error: undefined,\r\n      //ID para exclusão\r\n      deleteServiceId: -1,\r\n      //Campos para Pesquisa\r\n      bServico: \"\",\r\n      bValor: \"\",\r\n      bStatus: \"A\",\r\n      //Controle de Elementos HTML\r\n      showModal: false,\r\n      showModalDelete: false,\r\n      showModalUpdate: false,\r\n      showElement: false,\r\n    }\r\n  },\r\n  props: {\r\n\r\n  },\r\n  computed: {\r\n\r\n  },\r\n\r\n  methods: {\r\n\r\n    mostrarElemento: function () {\r\n      this.showElement = !this.showElement\r\n    },\r\n\r\n    ShowModal() {\r\n      this.showModal = true;\r\n    },\r\n    ShowModalDelete(id) {\r\n      console.log(\"Id do Serviço:\" + id);\r\n      this.deleteServiceId= id;\r\n      this.showModalDelete = true;\r\n    },\r\n    ShowModalUpdate(id) {\r\n      console.log(\"Id do Seviço:\" + id);\r\n      this.deleteServiceId = id;\r\n      this.showModalUpdate = true;\r\n\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };//validação da API\r\n      axios\r\n        .get(\"http://localhost:8686/api/service/\" + this.deleteServiceId, req)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.servico = res.data.servico;\r\n          this.valor = res.data.valor;\r\n          this.id = res.data.id;\r\n          this.status = res.data.status\r\n\r\n        }).catch(err => {\r\n          console.log(err.response);\r\n        });\r\n\r\n    },\r\n    HideModal() {\r\n      //fechar moldais\r\n      this.showModal = false;\r\n      this.showModalDelete = false;\r\n      this.showModalUpdate = false;\r\n\r\n      //limpar dados\r\n\r\n      this.id = \"\";\r\n      this.nome = \"\";\r\n      this.usuario = \"\";\r\n      this.email = \"\"\r\n\r\n    },\r\n    //===================CRUD===============================//\r\n    //inclusão\r\n    register() {\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };\r\n      axios\r\n        .post(\"http://localhost:8686/api/service\", {\r\n          servico: this.servico,\r\n          valor: this.valor,\r\n          status: \"A\"\r\n        }, req)\r\n        .then(res => {\r\n          this.$router.push({ name: 'service' });\r\n          console.log(res);\r\n        })\r\n        .catch(err => {\r\n          var msgErro = err.response.data.err;\r\n          this.error = msgErro\r\n          console.log(err)\r\n        })\r\n\r\n    },\r\n    //Exclusão\r\n    deleteService() {\r\n\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };\r\n\r\n      axios\r\n        .delete(\"http://localhost:8686/api/service/\" + this.deleteServiceId, req)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.showModalDelete = false;\r\n          // pesquisa\r\n          axios\r\n            .get(\"http://localhost:8686/api/service\", req)\r\n            .then((res) => {\r\n              this.servicos = res.data;\r\n              this.filteredServicos = res.data.filter(service => service.status.match(this.bStatus));\r\n              console.log(res);\r\n            })\r\n            .catch((err) => {\r\n              var msgErro = err.response.data.err;\r\n              this.error = msgErro;\r\n            });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        })\r\n    },//\r\n\r\n    //Edição\r\n    updateService() {\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };//validação da API\r\n      axios\r\n        .put(\"http://localhost:8686/api/service/\" + this.deleteServiceId, {\r\n          servico: this.servico,\r\n          valor: this.valor,\r\n          id: this.id,\r\n          status: this.status,\r\n        }, req)\r\n        .then(res => {\r\n          this.showModalUpdate = false;\r\n\r\n          // pesquisa\r\n          axios\r\n            .get(\"http://localhost:8686/api/service\", req)\r\n            .then((res) => {\r\n              this.servicos = res.data;\r\n              this.filteredServicos = res.data.filter(service => service.status.match(this.bStatus));\r\n              console.log(res);\r\n            })\r\n            .catch((err) => {\r\n              var msgErro = err.response.data.err;\r\n              this.error = msgErro;\r\n            });\r\n          console.log(res);\r\n\r\n        })\r\n        .catch(err => {\r\n          var msgErro = err.response.data.err;\r\n          this.error = msgErro\r\n          console.log(err)\r\n        })\r\n\r\n    },\r\n\r\n    buscarService: function () {\r\n      this.filteredServicos = this.servicos;\r\n      if (\r\n        this.bService != '' || this.bService != ' ' &&\r\n        this.bStatus != '' || this.bStatus != ' '\r\n      ) {\r\n        this.filteredServicos = this.servicos.filter(service => service.servico.match(this.bServico) && service.status.match(this.bStatus));\r\n\r\n      } else {\r\n        this.filteredServicos = this.servicos;\r\n      }\r\n\r\n\r\n    }\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"mappings":";;EAMSA,KAAK,EAAC;AAAc;;;;;;uBAL3BC,mBAAA,CA0BM,cAzBJC,mBAAA,6CAAgD,EAChDC,YAAA,CAAUC,iBAAA,GACVD,YAAA,CAAYE,mBAAA,GACZH,mBAAA,YAAe,EACfI,mBAAA,CAoBM,OApBNC,UAoBM,GAnBJD,mBAAA,CAkBO,eAjBLE,UAYM,EACNN,mBAAA,0BAA6B,EAG7BC,YAAA,CAA2CM,uBAAA;IAA5BC,OAAO,EAAEC,IAAA,CAAAC;EAAe,qC"},"metadata":{},"sourceType":"module","externalDependencies":[]}