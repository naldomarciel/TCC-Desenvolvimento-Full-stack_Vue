{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport NavBar from '@/components/inc/NavBar.vue';\nimport SideBar from '@/components/inc/SideBar.vue';\nexport default {\n  name: 'UsersView',\n  components: {\n    NavBar,\n    SideBar\n  },\n  created() {\n    var req = {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }; //Validação da API\n    // PESQUISA NO BANCO DE DADOS//\n    axios.get(\"http://localhost:8686/api/service\", req).then(res => {\n      this.servicos = res.data;\n      this.filteredServicos = res.data;\n      console.log(res);\n    }).catch(err => {\n      var msgErro = err.response.data.err;\n      this.error = msgErro;\n    }); //consulta de usuarios\n  },\n\n  data() {\n    return {\n      //consulta de dados do BD//\n      servicos: [],\n      filteredServicos: [],\n      //Variaveis\n      id: -1,\n      servico: \"\",\n      valor: \"\",\n      status: \"\",\n      error: undefined,\n      //ID para exclusão\n      deleteServiceId: -1,\n      //Campos para Pesquisa\n      bServico: \"\",\n      bValor: \"\",\n      bStatus: \"\",\n      //Controle de Elementos HTML\n      showModal: false,\n      showModalDelete: false,\n      showModalUpdate: false,\n      showElement: false\n    };\n  },\n  props: {},\n  computed: {},\n  methods: {\n    mostrarElemento: function () {\n      this.showElement = !this.showElement;\n    },\n    ShowModal() {\n      this.showModal = true;\n    },\n    ShowModalDelete(id) {\n      console.log(\"Id do Usuario:\" + id);\n      this.deleteUserId = id;\n      this.showModalDelete = true;\n    },\n    ShowModalUpdate(id) {\n      console.log(\"Id do Seviço:\" + id);\n      this.deleteServiceId = id;\n      this.showModalUpdate = true;\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }; //validação da API\n      axios.get(\"http://localhost:8686/api/service/\" + this.deleteServiceId, req).then(res => {\n        console.log(res);\n        this.servico = res.data.servico;\n        this.valor = res.data.valor;\n        this.id = res.data.id;\n        this.status = res.data.status;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    },\n    HideModal() {\n      //fechar moldais\n      this.showModal = false;\n      this.showModalDelete = false;\n      this.showModalUpdate = false;\n\n      //limpar dados\n\n      this.id = \"\";\n      this.nome = \"\";\n      this.usuario = \"\";\n      this.email = \"\";\n    },\n    //===================CRUD===============================//\n    //inclusão\n    register() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      };\n      axios.post(\"http://localhost:8686/api/service\", {\n        servico: this.servico,\n        valor: this.valor,\n        status: \"A\"\n      }, req).then(res => {\n        this.$router.push({\n          name: 'service'\n        });\n        console.log(res);\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n        console.log(err);\n      });\n    },\n    //Exclusão\n    deleteService() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      };\n      axios.delete(\"http://localhost:8686/api/service/\" + this.deleteServiceId, req).then(res => {\n        console.log(res);\n        this.showModalDelete = false;\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/service\", req).then(res => {\n          this.servicos = res.data;\n          this.filteredServicos = res.data;\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n        });\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //\n\n    //Edição\n    updateService() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }; //validação da API\n      axios.put(\"http://localhost:8686/api/service/\" + this.deleteServiceId, {\n        servico: this.servico,\n        valor: this.valor,\n        id: this.id,\n        status: this.status\n      }, req).then(res => {\n        this.showModalUpdate = false;\n\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/service\", req).then(res => {\n          this.servicos = res.data;\n          this.filteredServicos = res.data;\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n        });\n        console.log(res);\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n        console.log(err);\n      });\n    },\n    buscarService: function () {\n      this.filteredServicos = this.servicos;\n      if (this.bService != '' || this.bService != ' ' && this.bStatus != '' || this.bStatus != ' ') {\n        this.filteredServicos = this.servicos.filter(service => service.servico.match(this.bServico) && service.status.match(this.bStatus));\n      } else {\n        this.filteredServicos = this.servicos;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","NavBar","SideBar","name","components","created","req","headers","Authorization","localStorage","getItem","get","then","res","servicos","data","filteredServicos","console","log","catch","err","msgErro","response","error","id","servico","valor","status","undefined","deleteServiceId","bServico","bValor","bStatus","showModal","showModalDelete","showModalUpdate","showElement","props","computed","methods","mostrarElemento","ShowModal","ShowModalDelete","deleteUserId","ShowModalUpdate","HideModal","nome","usuario","email","register","post","$router","push","deleteService","delete","updateService","put","buscarService","bService","filter","service","match"],"sources":["C:\\Users\\naldo\\OneDrive\\Documents\\GitHub\\appbarber\\barbervue\\src\\views\\ServicesView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--Barras de navegação horizontal e vertical-->\r\n    <NavBar />\r\n    <Side-bar />\r\n    <!--Conteúdo-->\r\n    <div class=\"main-content\">\r\n      <main>\r\n        <div class=\"page-header\">\r\n          <div>\r\n            <h2>Cadastro de Serviços</h2>\r\n          </div>\r\n          <!--Migalhas de Pão - Navegalçao Histórica-->\r\n          <nav class=\"breadcrumb is-right is-small\" aria-label=\"breadcrumbs\">\r\n            <ul>\r\n              <li><a href=\"#\">Home</a></li>\r\n              <li><a href=\"#\">Cadastros</a></li>\r\n              <li class=\"is-active\"><a href=\"#\" aria-current=\"page\">Serviço</a></li>\r\n            </ul>\r\n          </nav>\r\n        </div>\r\n        <!--Cabeçalho de Pesquisa -->\r\n        <div class=\"card\" id=\"card-pesquisa\">\r\n          <header class=\"card-header\" id=\"header-card\">\r\n            <p class=\"card-header-title\">\r\n              <span class=\"icon\">\r\n                <i class=\"las la-filter\"></i>\r\n              </span>\r\n              <span>Filtros</span>\r\n            </p>\r\n            <button class=\"card-header-icon\" aria-label=\"more options\" @click=\"mostrarElemento\">\r\n              <span class=\"icon\">\r\n                <i :class=\"{ 'las la-angle-up': showElement, 'las la-angle-down': !showElement }\" aria-hidden=\"true\"></i>\r\n              </span>\r\n            </button>\r\n          </header>\r\n          <div class=\"card-content\" v-show=\"showElement\">\r\n            <div class=\"content\">\r\n\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <input class=\"input is-small\" type=\"text\" placeholder=\"ID\">\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <input class=\"input is-small\" type=\"text\" placeholder=\"Serviço\" v-model=\"bServico\">\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <div class=\"select is-small\">\r\n\r\n                      <select v-model=\"bStatus\">\r\n                        <option selected value=\"A\">Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <button class=\"button is-info is-small\" @click=\"buscarService\">Pesquisar</button>\r\n\r\n                </div>\r\n              </div>\r\n\r\n            </div>\r\n          </div>\r\n          <!--Pesquisas-->\r\n        </div>\r\n        <!--Cabeçalho do Formulario/tabela-->\r\n        <div class=\"card\">\r\n          <header class=\"card-header is-small\" id=\"header-card-form\">\r\n            <p class=\"card-header-title\">\r\n              <button class=\"button is-info is-outlined\" @click=\"ShowModal\">Adicionar</button>\r\n            </p>\r\n          </header>\r\n          <div class=\"card-content\">\r\n            <div class=\"content\">\r\n              <table class=\"table is-hoverable is-fullwidth\">\r\n                <thead class=\"is-fixed\">\r\n                  <tr>\r\n                    <th>ID</th>\r\n                    <th>Serviço</th>\r\n                    <th>Valor</th>\r\n                    <th>Status</th>\r\n                    <th>Ações</th>\r\n                  </tr>\r\n                </thead>\r\n\r\n                <tbody>\r\n                  <tr v-for=\"servico in filteredServicos\" :key=\"servico.id\">\r\n                    <td>{{ servico.id }}</td>\r\n                    <td>{{ servico.servico }}</td>\r\n                    <td>{{ servico.valor }}</td>\r\n                    <td>{{ servico.status }}</td>\r\n                    <td>\r\n                      <button class=\"button is-warning is-small\" @click=\"ShowModalUpdate(servico.id)\">Editar</button>\r\n                      <button class=\"button is-danger is-small\" @click=\"ShowModalDelete(servico.id)\">Deletar</button>\r\n                    </td>\r\n\r\n\r\n                  </tr>\r\n\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n          <footer class=\"card-footer\">\r\n\r\n          </footer>\r\n        </div>\r\n\r\n\r\n        <!--MODAL - Cadastro de Usuários-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModal }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Novo Serviço</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <div v-if=\"error != undefined\">\r\n                      <div class=\"notification is-danger\">\r\n                        {{ error }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input\" type=\"text\" placeholder=\"Nome Completo\" v-model=\"nome\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-user\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input input-small\" type=\"email\" placeholder=\"contato@mail.com\" v-model=\"email\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-envelope\"></i>\r\n                      </span>\r\n                    </p>\r\n\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input input-small\" type=\"text\" placeholder=\"Usuário\" v-model=\"usuario\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-user\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n\r\n                  <div class=\"field\">\r\n                    <p class=\"control has-icons-left\">\r\n                      <input class=\"input\" type=\"password\" placeholder=\"Senha\" v-model=\"senha\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-lock\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n\r\n\r\n                </div>\r\n              </div>\r\n\r\n\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"register\">Cadastrar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - Cadastro de Usuários-->\r\n        <!--MODAL - DELETE-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModalDelete }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n\r\n\r\n            <!-- Content ... -->\r\n\r\n\r\n            <div class=\"card\">\r\n              <header class=\"card-header\">\r\n                <p class=\"card-header-title is-centered\">\r\n                  ATENÇÃO\r\n                </p>\r\n\r\n              </header>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\">\r\n                  Deseja realmente Deletar?\r\n\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer\">\r\n                <a href=\"#\" class=\"card-footer-item\" @click=\"deleteUser\">Deletar</a>\r\n                <a href=\"#\" class=\"card-footer-item\" @click=\"HideModal\">Cancelar</a>\r\n              </footer>\r\n            </div>\r\n\r\n\r\n          </div>\r\n        </div> <!--MODAL - DELTE-->\r\n\r\n        <!--MODAL - ALTERAR-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModalUpdate }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Editar Serviço</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n\r\n                <div class=\"field-body\">\r\n\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <div v-if=\"error != undefined\">\r\n\r\n                      <div class=\"notification is-danger\">\r\n                        {{ error }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input\" type=\"text\" placeholder=\"Descrição do Serviço\" v-model=\"servico\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-user\"></i>\r\n                      </span>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded has-icons-left\">\r\n                      <input class=\"input input-small\" type=\"text\" placeholder=\"valor\" v-model=\"valor\">\r\n                      <span class=\"icon is-small is-left\">\r\n                        <i class=\"las la-envelope\"></i>\r\n                      </span>\r\n                    </p>\r\n\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <div class=\"select\">\r\n                      <select v-model=\"status\" >\r\n                        <option selected=\"A\" value=\"A\" >Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"updateService\">Salvar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - ALTERAR-->\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\n\r\nexport default {\r\n  name: 'UsersView',\r\n  components: {\r\n    NavBar,\r\n    SideBar,\r\n\r\n  },\r\n  created() {\r\n    var req = {\r\n      headers: {\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n      },\r\n    };//Validação da API\r\n    // PESQUISA NO BANCO DE DADOS//\r\n    axios\r\n      .get(\"http://localhost:8686/api/service\", req)\r\n      .then((res) => {\r\n        this.servicos = res.data;\r\n        this.filteredServicos = res.data;\r\n        console.log(res);\r\n      })\r\n      .catch((err) => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro;\r\n      });//consulta de usuarios\r\n\r\n\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      //consulta de dados do BD//\r\n      servicos: [],\r\n      filteredServicos: [],\r\n\r\n      //Variaveis\r\n      id: -1,\r\n      servico: \"\",\r\n      valor: \"\",\r\n      status: \"\",\r\n      error: undefined,\r\n      //ID para exclusão\r\n      deleteServiceId: -1,\r\n      //Campos para Pesquisa\r\n      bServico: \"\",\r\n      bValor: \"\",\r\n      bStatus: \"\",\r\n      //Controle de Elementos HTML\r\n      showModal: false,\r\n      showModalDelete: false,\r\n      showModalUpdate: false,\r\n      showElement: false,\r\n    }\r\n  },\r\n  props: {\r\n\r\n  },\r\n  computed: {\r\n\r\n  },\r\n\r\n  methods: {\r\n\r\n    mostrarElemento: function () {\r\n      this.showElement = !this.showElement\r\n    },\r\n\r\n    ShowModal() {\r\n      this.showModal = true;\r\n    },\r\n    ShowModalDelete(id) {\r\n      console.log(\"Id do Usuario:\" + id);\r\n      this.deleteUserId = id;\r\n      this.showModalDelete = true;\r\n    },\r\n    ShowModalUpdate(id) {\r\n      console.log(\"Id do Seviço:\" + id);\r\n      this.deleteServiceId = id;\r\n      this.showModalUpdate = true;\r\n\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };//validação da API\r\n      axios\r\n        .get(\"http://localhost:8686/api/service/\" + this.deleteServiceId, req)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.servico = res.data.servico;\r\n          this.valor = res.data.valor;\r\n          this.id = res.data.id;\r\n          this.status = res.data.status\r\n\r\n        }).catch(err => {\r\n          console.log(err.response);\r\n        });\r\n\r\n    },\r\n    HideModal() {\r\n      //fechar moldais\r\n      this.showModal = false;\r\n      this.showModalDelete = false;\r\n      this.showModalUpdate = false;\r\n\r\n      //limpar dados\r\n\r\n      this.id = \"\";\r\n      this.nome = \"\";\r\n      this.usuario = \"\";\r\n      this.email = \"\"\r\n\r\n    },\r\n    //===================CRUD===============================//\r\n    //inclusão\r\n    register() {\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };\r\n      axios\r\n        .post(\"http://localhost:8686/api/service\", {\r\n          servico: this.servico,\r\n          valor: this.valor,\r\n          status: \"A\"\r\n        }, req)\r\n        .then(res => {\r\n          this.$router.push({ name: 'service' });\r\n          console.log(res);\r\n        })\r\n        .catch(err => {\r\n          var msgErro = err.response.data.err;\r\n          this.error = msgErro\r\n          console.log(err)\r\n        })\r\n\r\n    },\r\n    //Exclusão\r\n    deleteService() {\r\n\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };\r\n\r\n      axios\r\n        .delete(\"http://localhost:8686/api/service/\" + this.deleteServiceId, req)\r\n        .then(res => {\r\n          console.log(res);\r\n          this.showModalDelete = false;\r\n          // pesquisa\r\n          axios\r\n            .get(\"http://localhost:8686/api/service\", req)\r\n            .then((res) => {\r\n              this.servicos = res.data;\r\n              this.filteredServicos = res.data;\r\n              console.log(res);\r\n            })\r\n            .catch((err) => {\r\n              var msgErro = err.response.data.err;\r\n              this.error = msgErro;\r\n            });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n        })\r\n    },//\r\n\r\n    //Edição\r\n    updateService() {\r\n      var req = {\r\n        headers: {\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n        },\r\n      };//validação da API\r\n      axios\r\n        .put(\"http://localhost:8686/api/service/\" + this.deleteServiceId, {\r\n          servico: this.servico,\r\n          valor: this.valor,\r\n          id: this.id,\r\n          status: this.status,\r\n        }, req)\r\n        .then(res => {\r\n          this.showModalUpdate = false;\r\n\r\n          // pesquisa\r\n          axios\r\n            .get(\"http://localhost:8686/api/service\", req)\r\n            .then((res) => {\r\n              this.servicos = res.data;\r\n              this.filteredServicos = res.data;\r\n              console.log(res);\r\n            })\r\n            .catch((err) => {\r\n              var msgErro = err.response.data.err;\r\n              this.error = msgErro;\r\n            });\r\n          console.log(res);\r\n\r\n        })\r\n        .catch(err => {\r\n          var msgErro = err.response.data.err;\r\n          this.error = msgErro\r\n          console.log(err)\r\n        })\r\n\r\n    },\r\n