n              </div>\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"register\">Cadastrar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - Cadastro de Usuários-->\r\n\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport FullCalendar from '@fullcalendar/vue3'\r\nimport dayGridPlugin from '@fullcalendar/daygrid'\r\nimport interactionPlugin from '@fullcalendar/interaction'\r\nimport timeGridPlugin from '@fullcalendar/timegrid'\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\n\r\nexport default {\r\n  components: {\r\n    FullCalendar, // make the <FullCalendar> tag available\r\n    NavBar,\r\n    SideBar,\r\n  },\r\n  data() {\r\n  \r\n    return {\r\n      calendarOptions: {\r\n        plugins: [dayGridPlugin, timeGridPlugin, interactionPlugin],\r\n        initialView: 'dayGridMonth',\r\n        editable: true,\r\n        selectable: true,\r\n        selectMirror: true,\r\n        dayMaxEvents: true,\r\n        locale: 'pt-br',\r\n        headerToolbar: {\r\n          left: 'prev,next',\r\n          center: 'title',\r\n          right: 'dayGridMonth,timeGridWeek,timeGridDay'// user can switch between the two\r\n        },\r\n\r\n       events: this.eventos[dados]\r\n\r\n      },\r\n      dados: [],\r\n      filteredUsuarios: [],\r\n      showModal: false,\r\n      showModalDelete: false,\r\n      showModalUpdate: false,\r\n      showElement: false,\r\n\r\n    }\r\n  },\r\n  created() {\r\n\r\n\r\n\r\n  },\r\n  methods: {\r\n    eventos(event){\r\n      var req = {\r\n      headers: {\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n      },\r\n    };//Validação da API\r\n    // PESQUISA NO BANCO DE DADOS//\r\n    axios\r\n      .get(\"http://localhost:8686/api/appointment\", req)\r\n      .then((res) => {\r\n        this.dados = res.data;\r\n        event=res.data;\r\n        console.log(res);\r\n      })\r\n      .catch((err) => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro;\r\n      });//consulta de usuarios\r\n\r\n    }\r\n\r\n    \r\n  }\r\n\r\n}\r\n</script>\r\n\r\n"],"mappings":"AAkGA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,aAAY,MAAO,uBAAsB;AAChD,OAAOC,iBAAgB,MAAO,2BAA0B;AACxD,OAAOC,cAAa,MAAO,wBAAuB;AAClD,OAAOC,MAAK,MAAO,6BAA4B;AAC/C,OAAOC,OAAM,MAAO,8BAA6B;AAEjD,eAAe;EACbC,UAAU,EAAE;IACVN,YAAY;IAAE;IACdI,MAAM;IACNC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IAEL,OAAO;MACLC,eAAe,EAAE;QACfC,OAAO,EAAE,CAACR,aAAa,EAAEE,cAAc,EAAED,iBAAiB,CAAC;QAC3DQ,WAAW,EAAE,cAAc;QAC3BC,QAAQ,EAAE,IAAI;QACdC,UAAU,EAAE,IAAI;QAChBC,YAAY,EAAE,IAAI;QAClBC,YAAY,EAAE,IAAI;QAClBC,MAAM,EAAE,OAAO;QACfC,aAAa,EAAE;UACbC,IAAI,EAAE,WAAW;UACjBC,MAAM,EAAE,OAAO;UACfC,KAAK,EAAE,uCAAuC;QAChD,CAAC;;QAEFC,MAAM,EAAE,IAAI,CAACC,OAAO,CAACC,KAAK;MAE3B,CAAC;MACDA,KAAK,EAAE,EAAE;MACTC,gBAAgB,EAAE,EAAE;MACpBC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE,KAAK;MACtBC,eAAe,EAAE,KAAK;MACtBC,WAAW,EAAE;IAEf;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG,CAIV,CAAC;EACDC,OAAO,EAAE;IACPR,OAAOA,CAACS,KAAK,EAAC;MACZ,IAAIC,GAAE,GAAI;QACVC,OAAO,EAAE;UACPC,aAAa,EAAE,SAAQ,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO;QACzD;MACF,CAAC,CAAC;MACF;MACApC,KAAI,CACDqC,GAAG,CAAC,uCAAuC,EAAEL,GAAG,EAChDM,IAAI,CAAEC,GAAG,IAAK;QACb,IAAI,CAAChB,KAAI,GAAIgB,GAAG,CAAC/B,IAAI;QACrBuB,KAAK,GAACQ,GAAG,CAAC/B,IAAI;QACdgC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB,CAAC,EACAG,KAAK,CAAEC,GAAG,IAAK;QACd,IAAIC,OAAM,GAAID,GAAG,CAACE,QAAQ,CAACrC,IAAI,CAACmC,GAAG;QACnC,IAAI,CAACG,KAAI,GAAIF,OAAO;MACtB,CAAC,CAAC,CAAC;IAEL;EAGF;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}