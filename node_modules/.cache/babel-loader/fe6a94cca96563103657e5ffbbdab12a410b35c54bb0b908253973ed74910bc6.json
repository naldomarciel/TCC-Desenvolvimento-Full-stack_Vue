{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      email: \"\",\n      senha: \"\",\n      nome: \"\",\n      error: undefined,\n      showElement: true,\n      showElementII: false\n    };\n  },\n  //criando método de registro\n  methods: {\n    login() {\n      axios.post(\"http://localhost:8686/api/login\", {\n        email: this.email,\n        senha: this.senha\n      }).then(res => {\n        console.log(res);\n        localStorage.setItem('token', res.data.token);\n        this.$router.push({\n          name: 'home'\n        });\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n      });\n    },\n    register() {\n      if (this.nome != '' || this.nome != ' ' && this.email != '' || this.email != ' ' && this.senha != '' || this.senha != ' ') {\n        axios.post(\"http://localhost:8686/api/user\", {\n          email: this.email,\n          senha: this.senha,\n          nome: this.nome,\n          status: \"A\"\n        }).then(res => {\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n          console.log(err);\n        });\n      }\n      {\n        alert(\"Informar Campos Obrigatórios\");\n        return;\n      }\n    },\n    mostrarElemento: function () {\n      this.showElement = !this.showElement;\n      this.showElementII = true;\n    },\n    mostrarElementoII: function () {\n      this.showElementII = !this.showElementII;\n      this.showElement = true;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","email","senha","nome","error","undefined","showElement","showElementII","methods","login","post","then","res","console","log","localStorage","setItem","token","$router","push","name","catch","err","msgErro","response","register","status","alert","mostrarElemento","mostrarElementoII"],"sources":["C:\\Users\\nal