{"ast":null,"code":"import axios from \"axios\";\nimport NavBar from '@/components/inc/NavBar.vue';\nimport SideBar from '@/components/inc/SideBar.vue';\nimport { excelParser } from \"../assets/js/excel-parser\";\nexport default {\n  name: 'ClientsView',\n  components: {\n    NavBar,\n    SideBar\n  },\n  mounted() {\n    this.getDataPagina(1);\n  },\n  created() {\n    var req = {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }; //Validação da API\n    // PESQUISA NO BANCO DE DADOS//\n    axios.get(\"http://localhost:8686/api/professional\", req).then(res => {\n      this.bd = res.data;\n      this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n      this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\n      console.log(res);\n    }).catch(err => {\n      var msgErro = err.response.data.err;\n      this.error = msgErro;\n    }); //consulta de usuarios\n  },\n\n  data() {\n    return {\n      //consulta de dados do BD//\n      bd: [],\n      filteredBd: [],\n      //Variaveis\n      id: -1,\n      profissional: \"\",\n      celular: \"\",\n      email: \"\",\n      status: \"A\",\n      error: undefined,\n      //ID para exclusão\n      deleteItemId: -1,\n      //Campos para Pesquisa\n      bProfissional: \"\",\n      bCelular: \"\",\n      bEmail: \"\",\n      bStatus: \"A\",\n      //Controle de Elementos HTML\n      showModal: false,\n      showModalDelete: false,\n      showModalUpdate: false,\n      showElement: false,\n      /*PAGINAÇÃO*/\n      elementosPorPagina: 10,\n      dadosPaginados: [],\n      paginaAtual: 1,\n      /*EXPORTAÇÃO*/\n      newFileName: \"Cadastro de Profissionais\"\n    };\n  },\n  props: {},\n  computed: {},\n  methods: {\n    mostrarElemento: function () {\n      this.showElement = !this.showElement;\n    },\n    totalPaginas() {\n      return Math.ceil(this.filteredBd.length / this.elementosPorPagina);\n    },\n    getDataPagina(numPagina) {\n      this.paginaAtual = numPagina;\n      this.dadosPaginados = [];\n      var ini = numPagina * this.elementosPorPagina - this.elementosPorPagina;\n      var fin = numPagina * this.elementosPorPagina;\n      this.dadosPaginados = this.filteredBd.slice(ini, fin);\n    },\n    getPreviusPage() {\n      if (this.paginaAtual > 1) {\n        this.paginaAtual--;\n      }\n      this.getDataPagina(this.paginaAtual);\n    },\n    getNextPage() {\n      if (this.paginaAtual < this.totalPaginas()) {\n        this.paginaAtual++;\n      }\n      this.getDataPagina(this.paginaAtual);\n    },\n    isActive(numPagina) {\n      return numPagina == this.paginaAtual ? 'is-current' : '';\n    },\n    ShowModal() {\n      this.showModal = true;\n    },\n    ShowModalDelete(id) {\n      console.log(\"Id do Serviço:\" + id);\n      this.deleteItemId = id;\n      this.showModalDelete = true;\n    },\n    ShowModalUpdate(id) {\n      console.log(\"Id do Seviço:\" + id);\n      this.deleteItemId = id;\n      this.showModalUpdate = true;\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }; //validação da API\n      axios.get(\"http://localhost:8686/api/professional/\" + this.deleteItemId, req).then(res => {\n        console.log(res);\n        this.profissional = res.data.profissional;\n        this.celular = res.data.celular;\n        this.email = res.data.email;\n        this.id = res.data.id;\n        this.status = res.data.status;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    },\n    HideModal() {\n      //fechar moldais\n      this.showModal = false;\n      this.showModalDelete = false;\n      this.showModalUpdate = false;\n\n      //limpar dados\n\n      this.id = \"\";\n      this.profissional = \"\";\n      this.celular = \"\";\n      this.email = \"\";\n    },\n    //===================CRUD===============================//\n    //inclusão\n    register() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      };\n      axios.post(\"http://localhost:8686/api/professional\", {\n        profissional: this.profissional,\n        celular: this.celular,\n        email: this.email,\n        status: \"A\"\n      }, req).then(res => {\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/professional\", req).then(res => {\n          this.bd = res.data;\n          this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n          this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\n          console.log(res);\n          alert(\"Cadastrado com Sucesso!\");\n          this.id = \"\";\n          this.profissional = \"\";\n          this.celular = \"\";\n          this.email = \"\";\n        });\n        console.log(res);\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n        console.log(err);\n      });\n    },\n    //Exclusão\n    deleteItem() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      };\n      axios.delete(\"http://localhost:8686/api/professional/\" + this.deleteItemId, req).then(res => {\n        console.log(res);\n        this.showModalDelete = false;\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/professional\", req).then(res => {\n          this.bd = res.data;\n          this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n          this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n        });\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //\n\n    //Edição\n    updateItem() {\n      var req = {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }; //validação da API\n      axios.put(\"http://localhost:8686/api/professional/\" + this.deleteItemId, {\n        profissional: this.profissional,\n        celular: this.celular,\n        email: this.email,\n        id: this.id,\n        status: this.status\n      }, req).then(res => {\n        this.showModalUpdate = false;\n\n        // pesquisa\n        axios.get(\"http://localhost:8686/api/professional\", req).then(res => {\n          this.bd = res.data;\n          this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\n          this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\n          console.log(res);\n        }).catch(err => {\n          var msgErro = err.response.data.err;\n          this.error = msgErro;\n        });\n        console.log(res);\n      }).catch(err => {\n        var msgErro = err.response.data.err;\n        this.error = msgErro;\n        console.log(err);\n      });\n    },\n    buscaAvc: function () {\n      this.dadosPaginados = this.bd;\n      if (this.bProfissional != '' || this.bProfissional != ' ' && this.bEmail != '' || this.bEmail != ' ' && this.bStatus != '' || this.bStatus != ' ') {\n        this.dadosPaginados = this.bd.filter(client => client.cliente.match(this.bCliente) && client.email.match(this.bEmail) && client.status.match(this.bStatus));\n      } else {\n        this.dadosPaginados = this.bd;\n      }\n    },\n    ExportExcel() {\n      excelParser().exportDataFromJSON(this.filteredBd, this.newFileName, \"xls\");\n    }\n  }\n};","map":{"version":3,"names":["axios","NavBar","SideBar","excelParser","name","components","mounted","getDataPagina","created","req","headers","Authorization","localStorage","getItem","get","then","res","bd","data","filteredBd","filter","status","match","bStatus","dadosPaginados","slice","elementosPorPagina","console","log","catch","err","msgErro","response","error","id","profissional","celular","email","undefined","deleteItemId","bProfissional","bCelular","bEmail","showModal","showModalDelete","showModalUpdate","showElement","paginaAtual","newFileName","props","computed","methods","mostrarElemento","totalPaginas","Math","ceil","length","numPagina","ini","fin","getPreviusPage","getNextPage","isActive","ShowModal","ShowModalDelete","ShowModalUpdate","HideModal","register","post","alert","deleteItem","delete","updateItem","put","buscaAvc","client","cliente","bCliente","ExportExcel","exportDataFromJSON"],"sources":["C:\\Users\\naldo\\OneDrive\\Documents\\GitHub\\appbarber\\barbervue\\src\\views\\ProfessionalsView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!--Barras de navegação horizontal e vertical-->\r\n    <NavBar />\r\n    <Side-bar />\r\n    <!--Conteúdo-->\r\n    <div class=\"main-content\">\r\n      <main>\r\n        <div class=\"page-header\">\r\n          <div>\r\n            <h2>Cadastro de Profissionais</h2>\r\n          </div>\r\n          <!--Migalhas de Pão - Navegalçao Histórica-->\r\n          <nav class=\"breadcrumb is-right is-small\" aria-label=\"breadcrumbs\">\r\n            <ul>\r\n              <li><a href=\"#\">Home</a></li>\r\n              <li><a href=\"#\">Cadastros</a></li>\r\n              <li class=\"is-active\"><a href=\"#\" aria-current=\"page\">Profissionais</a></li>\r\n            </ul>\r\n          </nav>\r\n        </div>\r\n        <!--Cabeçalho de Pesquisa -->\r\n        <div class=\"card\" id=\"card-pesquisa\">\r\n          <header class=\"card-header\" id=\"header-card\">\r\n            <p class=\"card-header-title\">\r\n              <span class=\"icon\">\r\n                <i class=\"las la-filter\"></i>\r\n              </span>\r\n              <span>Filtros</span>\r\n            </p>\r\n            <button class=\"card-header-icon\" aria-label=\"more options\" @click=\"mostrarElemento\">\r\n              <span class=\"icon\">\r\n                <i :class=\"{ 'las la-angle-up': showElement, 'las la-angle-down': !showElement }\" aria-hidden=\"true\"></i>\r\n              </span>\r\n            </button>\r\n          </header>\r\n          <div class=\"card-content\" v-show=\"showElement\">\r\n            <div class=\"content\">\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <input class=\"input is-small\" type=\"text\" placeholder=\"Profissional\" v-model=\"bProfissional\">\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <input class=\"input is-small\" type=\"e-mail\" placeholder=\"e-mail\" v-model=\"bEmail\">\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <div class=\"select is-small\">\r\n                      <select v-model=\"bStatus\">\r\n                        <option selected value=\"A\">Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                  <button class=\"button is-info is-small\" @click=\"buscaAvc\">Pesquisar</button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!--Pesquisas-->\r\n        </div>\r\n        <!--Cabeçalho do Formulario/tabela-->\r\n        <div class=\"card\">\r\n          <header class=\"card-header is-small\" id=\"header-card-form\">\r\n            <p class=\"card-header-title\">\r\n              <button class=\"button is-info is-outlined\" @click=\"ShowModal\">\r\n                <i class=\"las la-plus-circle\"> Adicionar </i> </button>\r\n              <button class=\"button is-info is-outlined\" @click=\"ExportExcel\">\r\n                <i class=\"las la-file-excel\"> Exportar </i>\r\n              </button>\r\n            </p>\r\n          </header>\r\n          <div class=\"card-content\">\r\n            <div class=\"content\">\r\n              <table class=\"table is-hoverable is-fullwidth\">\r\n                <thead class=\"is-fixed\">\r\n                  <tr>\r\n                    <th>Id</th>\r\n                    <th>Profissional</th>\r\n                    <th>Celular</th>\r\n                    <th>E-mail</th>\r\n                    <th>Status</th>\r\n                    <th>Ações</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"bd in dadosPaginados\" :key=\"bd.id\">\r\n                    <td>{{ bd.id }}</td>\r\n                    <td>{{ bd.profissional }}</td>\r\n                    <td>{{ bd.celular }}</td>\r\n                    <td>{{ bd.email }}</td>\r\n                    <td>{{ bd.status }}</td>\r\n                    <td>\r\n                      <button class=\"button is-warning is-small\" @click=\"ShowModalUpdate(bd.id)\">Editar</button>\r\n                      <button class=\"button is-danger is-small\" @click=\"ShowModalDelete(bd.id)\">Deletar</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n              <nav class=\"pagination is-centered is-rounded\" role=\"navigation\" aria-label=\"pagination\">\r\n                <a class=\"pagination-previous is-centered\" v-on:click=\"getPreviusPage()\"> <i\r\n                    class=\"las la-angle-left\"></i></a>\r\n                <a class=\"pagination-next\" v-on:click=\"getNextPage()\"><i class=\"las la-angle-right\"></i></a>\r\n                <ul class=\"pagination-list\">\r\n                  <li><a v-for=\"pagina in totalPaginas()\" :key=pagina.id v-on:click=\"getDataPagina(pagina)\"\r\n                      class=\"pagination-link\" v-bind:class=\"isActive(pagina)\" aria-label=\"Goto page 1\">{{ pagina }}</a>\r\n                  </li>\r\n                </ul>\r\n              </nav>\r\n            </div>\r\n          </div>\r\n          <footer class=\"card-footer\">\r\n\r\n          </footer>\r\n        </div>\r\n\r\n\r\n        <!--MODAL - Cadastro de Usuários-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModal }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Novo Profissional</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <div v-if=\"error != undefined\">\r\n                      <div class=\"notification is-danger\">\r\n                        {{ error }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <label class=\"label is-small\">Profissional:</label>\r\n                      <input class=\"input\" type=\"text\" placeholder=\"Descrição do Profissional\" v-model=\"profissional\">\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <label class=\"label is-small\">E-Mail:</label>\r\n                      <input class=\"input\" type=\"text\" placeholder=\"contato@mail\" v-model=\"email\">\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <label class=\"label is-small\">Celular:</label>\r\n                      <input class=\"input input-small\" type=\"text\" placeholder=\"(##) ####-####\" v-model=\"celular\">\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"register\">Cadastrar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - Cadastro de Usuários-->\r\n        <!--MODAL - DELETE-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModalDelete }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <!-- Content ... -->\r\n            <div class=\"card\">\r\n              <header class=\"card-header\">\r\n                <p class=\"card-header-title is-centered\">\r\n                  ATENÇÃO\r\n                </p>\r\n              </header>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\">\r\n                  Deseja realmente Deletar este Registro?\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer\">\r\n                <a href=\"#\" class=\"card-footer-item\" @click=\"deleteItem\">Deletar</a>\r\n                <a href=\"#\" class=\"card-footer-item\" @click=\"HideModal\">Cancelar</a>\r\n              </footer>\r\n            </div>\r\n          </div>\r\n        </div> <!--MODAL - DELTE-->\r\n\r\n        <!--MODAL - ALTERAR-->\r\n\r\n        <div :class=\"{ modal: true, 'is-active': showModalUpdate }\">\r\n          <div class=\"modal-background\"></div>\r\n          <div class=\"modal-card\">\r\n            <header class=\"modal-card-head\">\r\n              <p class=\"modal-card-title\">Editar Profissional</p>\r\n              <button class=\"delete\" aria-label=\"close\" @click=\"HideModal\"></button>\r\n            </header>\r\n            <section class=\"modal-card-body\">\r\n              <!-- Content ... -->\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <div v-if=\"error != undefined\">\r\n                      <div class=\"notification is-danger\">\r\n                        {{ error }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Nome do Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <label class=\"label is-small\">Profissional:</label>\r\n                      <input class=\"input\" type=\"text\" v-model=\"profissional\">\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"field is-horizontal\">\r\n                <div class=\"field-body\">\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <label class=\"label is-small\">E-mail:</label>\r\n                      <input class=\"input input-small\" type=\"email\" v-model=\"email\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <p class=\"control is-expanded\">\r\n                      <label class=\"label is-small\">Celular:</label>\r\n                      <input class=\"input input-small\" type=\"text\" v-model=\"celular\">\r\n                    </p>\r\n                  </div>\r\n                  <div class=\"field\"> <!--Usuario-->\r\n                    <label class=\"label is-small\">Status:</label>\r\n                    <div class=\"select\">\r\n                      \r\n                      <select v-model=\"status\">\r\n                        <option value=\"A\">Ativo</option>\r\n                        <option value=\"H\">Inativo</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </section>\r\n            <footer class=\"modal-card-foot\">\r\n              <button class=\"button is-success\" @click=\"updateItem\">Salvar</button>\r\n              <button class=\"button\" @click=\"HideModal\">Cancelar</button>\r\n            </footer>\r\n          </div>\r\n        </div> <!--MODAL - ALTERAR-->\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport NavBar from '@/components/inc/NavBar.vue'\r\nimport SideBar from '@/components/inc/SideBar.vue'\r\nimport { excelParser } from \"../assets/js/excel-parser\";\r\n\r\nexport default {\r\n  name: 'ClientsView',\r\n  components: {\r\n    NavBar,\r\n    SideBar,\r\n  },\r\n  mounted() {\r\n    this.getDataPagina(1);\r\n  },\r\n  created() {\r\n    var req = {\r\n      headers: {\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\r\n      },\r\n    };//Validação da API\r\n    // PESQUISA NO BANCO DE DADOS//\r\n    axios\r\n      .get(\"http://localhost:8686/api/professional\", req)\r\n      .then((res) => {\r\n        this.bd = res.data;\r\n        this.filteredBd = res.data.filter(bd => bd.status.match(this.bStatus));\r\n        this.dadosPaginados = this.filteredBd.slice(0, this.elementosPorPagina);\r\n        console.log(res);\r\n      })\r\n      .catch((err) => {\r\n        var msgErro = err.response.data.err;\r\n        this.error = msgErro;\r\n      });//consulta de usuarios\r\n\r\n\r\n  },\r\n  data() {\r\n    return {\r\n      //consulta de dados do BD//\r\n      bd: [],\r\n      filteredBd: [],\r\n\r\n      //Variaveis\r\n      id: -1,\r\n      profissional: \"\",\r\n      celular: \"\",\r\n      email: \"\",\r\n      status: \"A\",\r\n  \r\n      error: undefined,\r\n      //ID para exclusão\r\n      deleteItemId: -1,\r\n      //Campos para Pesquisa\r\n      bProfissional: \"\",\r\n      bCelular: \"\",\r\n      bEmail: \"\",\r\n      bStatus: \"A\",\r\n      //Controle de Elementos HTML\r\n      showModal: false,\r\n      showModalDelete: false,\r\n      showModalUpdate: false,\r\n      showElement: false,\r\n      /*PAGINAÇÃO*/\r\n      elementosPorPagina: 10,\r\n      dadosPaginados: [],\r\n      paginaAtual: 1,\r\n     /*EXPORTAÇÃO*/\r\n      newFileName:\"Cadastro de Profissionais\"\r\n    }\r\n  },\r\n  props: {\r\n\r\n  },\r\n  computed: {\r\n\r\n  },\r\n\r\n  methods: {\r\n\r\n    mostrarElemento: function () {\r\n      this.showElement = !this.showElement\r\n    },\r\n    totalPaginas() {\r\n      return Math.ceil(this.filteredBd.length / this.elementosPorPagina)\r\n    },\r\n    getDataPagina(numPagina) {\r\n      this.paginaAtual = numPagina;\r\n      this.dadosPaginados = [];\r\n      var ini = (numPagina * this.elementosPorPagina) - this.elementosPorPagina;\r\n      var fin = (numPagina * this.elementosPorPagina);\r\n      this.dadosPaginados = this.filteredBd.slice(ini, fin);\r\n    },\r\n    getPreviusPage() {\r\n      if (this.paginaAtual > 1) {\r\n        this.paginaAtual--;\r\n      }\r\n      this.getDataPagina(this.paginaAtual)\r\n    },\r\n    getNextPage() {\r\n      if (this.paginaAtual < this.totalPaginas()) {\r\n        this.pagina